# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

# Home page
GET         /                    controllers.Application.index(s : String ?= "")

GET         /livredor            controllers.Application.livreDOr()
POST        /livredor            controllers.Application.submitForm()

GET         /tmpImage            controllers.Application.getTmpImage()


#GET        /test/:name          controllers.Application.test1(name)
#GET        /test2               controllers.Application.test1(name="dfdf")



# Map static resources from the /public folder to the /assets URL path
GET         /assets/*file        controllers.Assets.at(path="/public", file)
